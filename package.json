{
	"name": "my-links",
	"version": "4.1.0",
	"type": "module",
	"license": "GPL-3.0-only",
	"scripts": {
		"start": "node bin/server.js",
		"build": "node ace build",
		"dev": "node ace serve --hmr",
		"test": "node ace test",
		"lint": "eslint . --report-unused-disable-directives --max-warnings 0 --ignore-pattern '.adonisjs/*'",
		"format": "prettier --write --parser typescript '**/*.{ts,tsx}'",
		"typecheck": "tsc --noEmit",
		"check": "pnpm run lint && pnpm run typecheck",
		"prepare": "husky",
		"release": "release-it",
		"tuyau": "node ace tuyau:generate",
		"extract": "lingui extract --clean",
		"compile": "lingui compile --typescript"
	},
	"imports": {
		"#adonis/api": "./.adonisjs/api.ts",
		"#config/*": "./config/*.js",
		"#controllers/*": "./app/controllers/*.js",
		"#dtos/*": "./app/dtos/*.js",
		"#models/*": "./app/models/*.js",
		"#routes/*": "./app/routes/*.js",
		"#services/*": "./app/services/*.js",
		"#validators/*": "./app/validators/*.js",
		"#middlewares/*": "./app/middlewares/*.js",
		"#enums/*": "./app/enums/*.js",
		"#types/*": "./app/types/*.js",
		"#exceptions/*": "./app/exceptions/*.js",
		"#lib/*": "./app/lib/*.js",
		"#constants/*": "./app/constants/*.js",
		"#providers/*": "./providers/*.js",
		"#database/*": "./database/*.js",
		"#tests/*": "./tests/*.js",
		"#shared/*": "./shared/*.js",
		"#start/*": "./start/*.js"
	},
	"devDependencies": {
		"@adonisjs/assembler": "^7.8.2",
		"@adonisjs/eslint-config": "2.1.2",
		"@adonisjs/prettier-config": "^1.4.5",
		"@adonisjs/tsconfig": "^1.4.1",
		"@faker-js/faker": "^10.2.0",
		"@japa/assert": "^4.2.0",
		"@japa/plugin-adonisjs": "^4.0.0",
		"@japa/runner": "^5.3.0",
		"@lingui/babel-plugin-lingui-macro": "^5.9.0",
		"@lingui/cli": "^5.9.0",
		"@lingui/vite-plugin": "^5.9.0",
		"@tuyau/utils": "^0.0.9",
		"@types/luxon": "^3.7.1",
		"@types/node": "^24.10.0",
		"@types/react": "^19.2.9",
		"@types/react-dom": "^19.2.3",
		"@typescript-eslint/eslint-plugin": "^8.54.0",
		"@unocss/preset-icons": "^66.6.0",
		"@unocss/preset-web-fonts": "^66.6.0",
		"@vitejs/plugin-react": "^5.1.2",
		"babel-plugin-macros": "^3.1.0",
		"eslint": "^9.39.2",
		"eslint-plugin-lingui": "^0.11.0",
		"hot-hook": "^0.4.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"pino-pretty": "^13.1.3",
		"prettier": "^3.8.1",
		"release-it": "^19.2.4",
		"ts-node-maintained": "^10.9.6",
		"typescript": "~5.9.3",
		"unocss": "^66.6.0",
		"vite": "^7.3.1"
	},
	"dependencies": {
		"@adonisjs/ally": "^5.1.1",
		"@adonisjs/auth": "^9.5.1",
		"@adonisjs/core": "^6.19.3",
		"@adonisjs/cors": "^2.2.1",
		"@adonisjs/inertia": "^3.1.1",
		"@adonisjs/lucid": "^21.8.2",
		"@adonisjs/session": "^7.7.1",
		"@adonisjs/shield": "^8.2.0",
		"@adonisjs/static": "^1.1.1",
		"@adonisjs/vite": "^4.0.0",
		"@inertiajs/react": "^2.3.11",
		"@lingui/core": "^5.9.0",
		"@lingui/react": "^5.9.0",
		"@swc/core": "^1.15.10",
		"@tuyau/client": "^0.2.10",
		"@tuyau/core": "^0.4.2",
		"@tuyau/inertia": "^0.0.15",
		"@vinejs/vine": "^4.2.0",
		"bentocache": "^1.6.0",
		"clsx": "^2.1.1",
		"edge.js": "^6.4.0",
		"emoji-picker-react": "^4.17.1",
		"emoji-regex": "^10.6.0",
		"knex": "^3.1.0",
		"luxon": "^3.7.2",
		"node-html-parser": "^7.0.2",
		"pg": "^8.17.2",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"reflect-metadata": "^0.2.2",
		"zustand": "^5.0.10"
	},
	"hotHook": {
		"boundaries": [
			"./app/controllers/**/*.ts",
			"./app/middlewares/**/*.ts"
		]
	},
	"prettier": {
		"trailingComma": "es5",
		"semi": true,
		"singleQuote": true,
		"useTabs": true,
		"quoteProps": "as-needed",
		"bracketSpacing": true,
		"arrowParens": "always",
		"printWidth": 80
	},
	"lint-staged": {
		"*.js,*.ts,*.jsx,*.tsx": "eslint --cache --fix"
	},
	"volta": {
		"node": "24.13.0"
	}
}
